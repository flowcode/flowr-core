<body class="sidebar-mini skin-black-light">
{% block wrapper %}{% endblock %}

<div class="modal fade" id="modal_flowr_info" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
                <h4 class="modal-title" id="myModalLabel">{{ 'about'|trans|title }}</h4>
            </div>
            <div class="modal-body">
                <div class="row about">
                    <div class="col-sm-5 text-right">
                        <img src="{{ asset('bundles/flowercore/images/flowr_square.png') }}" class="img-responsive">
                    </div>
                    <div class="col-sm-7">
                        <h4>Flowr</h4>
                        <p>
                            Version <span class="flowr-version">v{{ flowr_version }}</span>
                        </p>
                        <p>
                            {{'Flowr is builded by' | trans}} <a href="https://flowcode.com.ar">Flowcode</a>.
                        </p>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                {{'More info at' | trans}} <a href="http://myflowr.con" target="_blank">myflowr.con</a>
            </div>
        </div>
    </div>
</div>

